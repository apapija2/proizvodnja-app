<%- include('layout', { title: 'Å ifranti' }) %>

<h1><%= title %></h1>
<form action="/sifrant-<%= type %>" method="POST">
  <div class="form-group">
    <label for="<%= type %>">Unesite <%= label %>:</label>
    <input type="text" class="form-control" id="<%= type %>" name="<%= type %>" required>
  </div>
  <button type="submit" class="btn btn-primary mt-3">Dodaj</button>
</form>

<ul class="mt-4">
  <% items.forEach(item => { %>
    <li><%= item.ime %></li>
  <% }) %>
</ul>
